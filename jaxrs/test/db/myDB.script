CREATE MEMORY TABLE CUSTOMER(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CITY VARCHAR(255),COUNTRY VARCHAR(255),FIRSTNAME VARCHAR(255),LASTNAME VARCHAR(255),STATE VARCHAR(255),STREET VARCHAR(255),ZIP VARCHAR(255))
CREATE MEMORY TABLE LINEITEM(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,QUANTITY INTEGER NOT NULL,PRODUCT_ID INTEGER)
CREATE MEMORY TABLE PRODUCT(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,COST DOUBLE NOT NULL,NAME VARCHAR(255))
CREATE MEMORY TABLE PURCHASEORDER(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CANCELLED BOOLEAN NOT NULL,DATE VARCHAR(255),TOTAL DOUBLE NOT NULL,CUSTOMER_ID INTEGER,CONSTRAINT FKDFD4BAADEB02F21F FOREIGN KEY(CUSTOMER_ID) REFERENCES CUSTOMER(ID))
CREATE MEMORY TABLE PURCHASEORDER_LINEITEM(PURCHASEORDER_ID INTEGER NOT NULL,LINEITEMS_ID INTEGER NOT NULL,CONSTRAINT SYS_CT_54 UNIQUE(LINEITEMS_ID),CONSTRAINT FK7092129976F8D7A FOREIGN KEY(LINEITEMS_ID) REFERENCES LINEITEM(ID),CONSTRAINT FK70921299608F081C FOREIGN KEY(PURCHASEORDER_ID) REFERENCES PURCHASEORDER(ID))
